   "metadata": {},
   "outputs": [],
   "source": [
    "def ohe_le(x,y):\n",
    "    le.fit(x)\n",
    "    tr = le.transform(y)\n",
    "    d = [[i,e ]for i,e in zip(['h','e','l','l','o'],tr)]\n",
    "    enc.fit(d)\n",
    "    return enc.transform(d).toarray()[:,:len(y)],tr"
   ]
